doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Ratings 
        link(rel="stylesheet", href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css")

    body 
         div 
            table(Class="table")
                thead 
                    tr 
                        th Name 
                        th Image
                        th Rating 
                        th Review
                tbody 
                    each ra in rat
                        tr 
                            th=ra.fullname
                            th 
                            
                            \th 
                              img(src=ra.image, class=["rounded", "w-25"])
                            th=ra.rating
                            th=ra.review 

